.fa-icon
    vertical-align middle
    padding 0 4px

.el-table-custom td:first-child,
.el-table-custom th:first-child
  text-align center
.el-table__header-wrapper thead th
  background-color #f6f6f6
  color #1f2d3d
.th .el-checkbox,
.td .el-checkbox
  text-align center
.el-table th,
.el-table td
    padding 4px 0
.el-table th
    font-weight 500
th.column
  text-align center

.el-pagination__editor
  width 70px

//dialog
.el-dialog__header{
  height: 34px;
  border-bottom: 1px solid #e0e0e0;
}
.el-dialog__body
  padding:30px 70px
  .el-tabs
    margin-top:-69px
    .el-tabs__nav
      left: 50%;
      margin-left: -125px;
      .el-tabs__item.is-active
        color: #333
      .el-tabs__active-bar
        background-color:red
  .twoTabs.el-tabs //两个tab
    .el-tabs__nav
      margin-left: -85px;
  .fourTabs.el-tabs //四个tab
    .el-tabs__nav
      margin-left: -180px;
  .fiveTabs.el-tabs //五个tab
    .el-tabs__nav
      margin-left: -280px;

.el-dialog__headerbtn .el-dialog__close{
  color:red
}
//带滚动条的对话框
.scrollDialog .el-dialog__body{
  padding: 30px 0;
}
.scrollDialog .el-tabs__content{
  padding: 0 70px;
  box-sizing: border-box;
  height: 400px;
  overflow-y: auto;
}
.scrollDialog .el-dialog__headerbtn{
  position: absolute;
  z-index: 1;
  right: 20px;
}

.dialogHeader
  margin-top: -86px
  height: 55px
  line-height: 55px
  text-align: center
  margin-bottom:20px
  span
    display: inline-block
    height: 52px
    border-bottom:3px solid #e42f3b
    min-width:62px

.el-dialog__footer{
  text-align: center;
  border-top: 1px solid #e0e0e0;
}
